    "l = 100\n",
    "dX = 1\n",
    "dt = 0.01\n",
    "D = 1\n",
    "r = 0.64\n",
    "B = 75\n",
    "ngrid = int(l / dX) + 1  # number of grid points\n",
    "X_all = np.arange(0, ngrid) * dX  # X values for each grid point\n",
    "\n",
    "u0 = np.zeros(ngrid)\n",
    "u0[int(ngrid / 2) - 2: int(ngrid / 2) + 3] = 50\n",
    "\n",
    "def fisher(t, u, r, B):\n",
    "    return r * u * (1 - u / B)\n",
    "\n",
    "plt.figure(figsize=(6,3))\n",
